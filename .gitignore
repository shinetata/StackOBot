*.xcodeproj
*.xcworkspace
*.sln
*.suo
*.opensdf
*.sdf
*.VC.db
*.VC.opendb

# Precompiled Assets
SourceArt/**/*.png
SourceArt/**/*.tga

# Binary Files
Binaries/*
Plugins/*/Binaries/*

# Builds
Build/*

# Whitelist PakBlacklist-<BuildConfiguration>.txt files
!Build/*/
Build/*/**
!Build/*/PakBlacklist*.txt

# Don't ignore icon files in Build
!Build/**/*.ico

# Built data for maps
*_BuiltData.uasset

# Configuration files generated by the Editor
Saved/

# Compiled source files for the engine to use
Intermediate/
Plugins/**/Intermediate/

# Cache files for the editor to use
DerivedDataCache/

# Unreal Engine asset/level externalization (should generally be versioned when used).
# If you *intentionally* want to ignore them, uncomment the following lines.
# Content/__ExternalActors__/
# Content/__ExternalObjects__/
# Content/__ExternalData__/

# UnrealCSharp plugin: keep source (submodule), ignore build outputs
Plugins/UnrealCSharp/**/Binaries/
Plugins/UnrealCSharp/**/Intermediate/
Plugins/UnrealCSharp/**/Saved/

# All plugins: ignore build outputs
Plugins/**/Binaries/
Plugins/**/Intermediate/
Plugins/**/Saved/

# UnrealCSharp C# build outputs (generated)
Content/Script/

# C#/.NET build artifacts (Script toolchain)
Script/**/bin/
Script/**/obj/
Script/**/.vs/
Script/**/TestResults/

# UnrealCSharp generated proxies (usually regeneratable)
Script/**/Proxy/

# UnrealCSharp local toolchain projects (generated; not versioned in this repo)
Script/CodeAnalysis/
Script/SourceGenerator/
Script/UE/
Script/Weavers/
Script/Game/*.csproj
Script/Game/*.csproj.*
Script/Game/FodyWeavers.xml
Script/Game/FodyWeavers.xsd
Script/Game/*.props
